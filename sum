#include<stdio.h>
int main(){
int matris[3][3];
int i,j;
int sum = 0;


for (i = 0; i < 3 ; i++)
{
    for ( j = 0; j < 3; j++)
    {
        scanf("%d",&matris[i][j]);
    }
    
    printf("\n");
}

for (j = 0; j < 3; j++)
{
    for (i = 0; i < 3; i++)
    {
        sum += matris[i][j];
    }
    
    printf("%d\t",sum);

    sum = 0;
        
}

    return 0;
}
